<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>chat-app</title>
    <link rel="stylesheet" type="text/css" href="./styles/index.css">
    <link rel="stylesheet" href="https://fonts.googleapis.com/icon?family=Material+Icons">
    <script src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.dev.js"></script>
</head>
<body>
    <div class="page">
   <nav>
       <div class="title">Chat-app</div>
    <ul>
        <li><a href=""><i class="material-icons">person</i></a></li>
        <li><a href=""><i class="material-icons">delete</i></a></li>
        <li><a href="/search-freind"><i class="material-icons">search</i></a></li>
        <li><a><i class="material-icons">menu</i></a></li>
    </ul>
   </nav>
   <div id="myemail"><%=myemail%></div>
  <div class="container">
        <div class="friends">
            <% friends.forEach(friend=>{ %>
            <div class="line"></div>
            <div class="friend" id="<%=friend.email%>">
                <a id="a"+<%=friend.email%> href="">
                <div class="sameline">
                    <div class="image">
                         <img src="https://www.kindpng.com/picc/m/495-4952535_create-digital-profile-icon-blue-user-profile-icon.png">
                    </div>
                    <div class="nextline">
                        <div class="sameline">
                            <div class="name"><%= friend.name %></div>
                            <div class="online" id="status"+<%=friend.email%> ></div>
                        </div>
                     <div class="previous-msg">hey how are you</div>
                     </div>
                </div>
                 </a>
            </div>
        <div class="line"></div>
        <%}) %>
            
        </div>
    </div>
</div>
<script src="./scripts/index.js"></script>
</body>
</html>